{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "env": {
        "API_KEY": "@API_KEY",
        "ZEROBOUNCE_API_KEY": "@ZEROBOUNCE_API_KEY"
    }
}